{
    "name": "webcoast/user-manual",
    "title": "User manual",
    "description": "TYPO3 extension for rendering a user manual in the backend",
    "keywords": ["typo3", "cms", "documentation", "render", "manual", "usermanual"],
    "type": "typo3-cms-extension",
    "minimum-stability": "dev",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Thorben Nissen",
            "email": "thorben@webcoast.dk"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "typo3/cms": {
            "extension-key": "user_manual"
        }
    },
    "autoload": {
        "psr-4": {
            "WEBcoast\\UserManual\\": "Classes"
        }
    },
    "require": {
        "phpdocumentor/guides": "^1.2",
        "phpdocumentor/guides-restructured-text": "^1.2",
        "phpdocumentor/guides-graphs": "^1.2",
        "t3docs/typo3-docs-theme": "^0.2",
        "t3docs/typo3-guides-extension": "^0.2",
        "typo3/cms-core": "^12.4"
    }
}
